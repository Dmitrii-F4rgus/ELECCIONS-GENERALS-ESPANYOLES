import mysql.connector

# Connect to the MySQL server
cnx = mysql.connector.connect(user='username', password='password',
                              host='hostname',
                              database='database_name')

# Create a cursor object
cursor = cnx.cursor()

# Define the data
data = "0220191111111111111000000"

# Extract the values from the data string
tipo_eleccion = data[0:2]
year = data[2:6]
mes = data[6:8]
vuelta = data[8:9]
tipo_ambito = data[9:10]
ambito_territorial = data[10:12]
dia = data[12:14]
mes_eleccion = data[14:16]
anio_eleccion = data[16:20]
hora_apertura = data[20:25]
hora_cierre = data[25:30]
hora_avance1 = data[30:35]
hora_avance2 = data[35:40]

# Insert the data into the table
query = "INSERT INTO identificacion (tipo_eleccion, year, mes, vuelta, tipo_ambito, ambito_territorial, dia, mes_eleccion, anio_eleccion, hora_apertura, hora_cierre, hora_avance1, hora_avance2) VALUES (%s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s, %s)"
values = (tipo_eleccion, year, mes, vuelta, tipo_ambito, ambito_territorial, dia, mes_eleccion, anio_eleccion, hora_apertura, hora_cierre, hora_avance1, hora_avance2)
cursor.execute(query, values)

# Commit the changes
cnx.commit()

# Close the cursor and connection
cursor.close()
cnx.close()
